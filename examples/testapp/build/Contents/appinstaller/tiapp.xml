<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org" xmlns:appc="http://www.appcelerator.org">
   <id>com.titaniumapp.installer.app</id>
   <name>Appcelerator Titanium Application Installer</name>
   <version>0.1</version>
   <window>
      <id>initial</id>
      <title>Application Installation</title>
      <url>app://index.html</url>
      <width>600</width>
      <height>200</height>
	  <fullscreen>false</fullscreen>
	  <maximizable>false</maximizable>
	  <minimizable>false</minimizable>
	  <closeable>true</closeable>
	  <resizable>false</resizable>
	  <chrome scrollbars="false">true</chrome>
   </window>
</ti:app>
